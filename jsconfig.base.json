{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"forceConsistentCasingInFileNames": true,
		"importsNotUsedAsValues": "error",
		"isolatedModules": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"preserveValueImports": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ESNext"
	}
}
